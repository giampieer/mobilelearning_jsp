<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="campuscampus" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>JPA.Laboratorio</class>
    <class>JPA.Administrador</class>
    <class>JPA.Anuncios</class>
    <class>JPA.Recursos</class>
    <class>JPA.Alumno</class>
    <class>JPA.Asistencia</class>
    <class>JPA.Pagos</class>
    <class>JPA.Reservalaboratorio</class>
    <class>JPA.Profesor</class>
    <class>JPA.Actividad</class>
    <class>JPA.Notas</class>
    <class>JPA.MatriculaHasCurso</class>
    <class>JPA.Horario</class>
    <class>JPA.Reservalibro</class>
    <class>JPA.Matricula</class>
    <class>JPA.Libro</class>
    <class>JPA.Curso</class>
    <class>JPA.Reservaactividad</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/bdcampus?zeroDateTimeBehavior=convertToNull"/>
      <property name="javax.persistence.jdbc.user" value="root"/>
      <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value="root"/>
      <property name="javax.persistence.schema-generation.database.action" value="create"/>
    </properties>
  </persistence-unit>
</persistence>
